{
    "manifest_version": 2,
    "name": "Read QR code from Clipboard",
    "version": "0.0.0.1",
    "icons": {
        "128": "icons/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icons/icon.png"
        },
        "default_title": "Read QR code from Clipboard",
        "default_popup": "popup/popup.html",
        "browser_style": false
    },
    "permissions": [
        "tabs",
        "clipboardRead",
        "clipboardWrite"
    ]
}